#include<stdio.h>
int main() {
  int n,c=0,t;
  scanf("%d",&t);
  while(t>0)
  {
  scanf("%d",&n);
  while(n>0)
  {
      if(n>3)
      {
          n=n-3;
          c++;
      }
      else if(n==3)
      {
          n=n-2;
          c++;
      }
      else
      {
          n=n-1;
          c++;
      }
  }
  if(c%2==0)
  {
      printf("Yes\n");
  }
  else
  {
      printf("No\n");
  }
  t--;
  c=0;
  }
  return 0;
 
}
